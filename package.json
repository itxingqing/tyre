{
  "name": "vue-project-template",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/seasonTree/vue-project-template.git",
  "license": "MIT",
  "scripts": {
    "dev": "node ./node_modules/webpack/bin/webpack.js --config ./client/webpack/webpack.config.dev.js",
    "dev-watch": "node ./node_modules/webpack/bin/webpack.js --watch --config ./client/webpack/webpack.config.dev.js",
    "server-dev": "export NODE_ENV=development && node ./server/index.js",
    "server-watch": "node ./serverAutoStart.js",
    "server": "export NODE_ENV=production && node ./server/index.js",
    "build": "webpack --config ./client/webpack/webpack.config.pro.js",
    "start": "node ./node_modules/concurrently/src/main.js \"yarn server-watch\" \"yarn dev-watch\"",
    "public": "yarn install --production"
  },
  "dependencies": {
    "ejs": "^2.6.1",
    "element-ui": "^2.4.6",
    "koa": "^2.5.2",
    "koa-body": "^4.0.4",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.1.4",
    "lodash": "^4.17.10",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.38.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "autoprefixer": "^9.1.3",
    "babel-loader": "^8.0.2",
    "concurrently": "^3.6.1",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "style-loader": "^0.22.1",
    "tree-kill": "^1.2.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "vue-loader": "^15.4.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  }
}
